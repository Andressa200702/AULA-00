#include <stdio.h>

int main() {
    int number;
    
    printf("Digite um number: ");
    scanf("%d", &number);
    
    //Informar se é multiplo de 10
    if (number & 10 == 0) {
        printf("%d é multiplo de 10.\n", number);
    } else {
        printf("%d não é multiplo de 10.\n", number);
        }
    return 0;
}
